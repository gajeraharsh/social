# Copy to .env and update values
PORT=3000
MONGO_URI=mongodb://127.0.0.1:27017/social_admin
SESSION_SECRET=change_this_secret
## Base URL of this server (used to build public upload URLs)
# BASE_URL=http://localhost:3000
## Optional: override schedule (comma-separated cron expressions)
# Example runs at 00:00, 04:00, 08:00, 12:00, 16:00 daily by default, so leave empty to use defaults
# POST_SCHEDULE_CRONS="0 0 * * *,0 4 * * *,0 8 * * *,0 12 * * *,0 16 * * *"
## Optional: number of accounts processed in parallel each tick
# POST_ACCOUNT_CONCURRENCY=10
